{
    "name": "juanperez/sistema-reserva-deporte",
    "description": "Un sistema para realizar reservas de espacios deportivos.",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.4",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "monolog/monolog": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
